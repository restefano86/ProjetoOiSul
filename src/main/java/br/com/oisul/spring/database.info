CREATE TABLE `projetooisul`.`empresa` (
  `idEmpresa` INT NOT NULL AUTO_INCREMENT,
  `idUsuario` INT NULL,
  `deRazaoSocial` VARCHAR(80) NULL,
  `nuCnpj` INT NULL,
  `nmRepLegal` VARCHAR(80) NULL,
  `nuCpfRepLegal` INT NULL,
  `nuDiaVencimento` INT NULL,
  `deEndereco` VARCHAR(80) NULL,
  `nmBairro` VARCHAR(80) NULL,
  `nuCep` INT NULL,
  `nmMunicipio` VARCHAR(80) NULL,
  `deUf` VARCHAR(2) NULL,
  `nuDddFixo` INT NULL,
  `nuFixo` INT NULL,
  `nuDddCelular` INT NULL,
  `nuCelular` INT NULL,
  `deEmail` VARCHAR(80) NULL,
  `deEnderecoCob` VARCHAR(80) NULL,
  `nmBairroCob` VARCHAR(80) NULL,
  `nmMunicipioCob` VARCHAR(80) NULL,
  `deUfCob` VARCHAR(2) NULL,
  `nuCepCob` INT NULL,
  PRIMARY KEY (`idEmpresa`));